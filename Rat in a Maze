// Online C++ compiler to run C++ program online
#include <iostream>
void solveMaze(int arr[3][3],int row,int srcx,int srcy, vector<vector<bool> &visited, vector<string>&path, string output)
{
    if(srcx==row-1 && srcy==col-1){
        path.push_back(output);
        return;
    }
    
    //down -> i+1, j
    if(isSafe(i+1,j,row,col,arr,visited)){
        visited[i+1][j]=true;
        solveMaze(arr,row,col,i+1,j,visited,path,output+'D');
        //backtrack
        visited[i+1][j]=false;
    }
    
    //left-> i,j-1
    if(isSafe(i,j-1,row,col,arr,visited)){
        visited[i][j-1]=true;
        solveMaze(arr,row,col,i,j-1,visited,path,output+'L');
        //backtrack
        visited[i][j-1];
    }
    
}
int main() {
    // Write C++ code here
   int maze[3][3]={
       {1,0,0},
       {1,1,0},
       {1,1,1}
   };
   int rows=3;
   int col=3;
   vector <vector<bool>> visited(row,vector<bool>(col,false));
   //src ki value ko true mark kar dete hai
   visited[0][0]=true;
   vector <string> path;
   string output="";
   solveMaze(arr,row,0,0,visited,path,output);
   

    return 0;
}
