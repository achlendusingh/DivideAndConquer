#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
int arr1[]={34,25};
int arr2[]={45,34,56,32,23};
int arr3[50];
int i=0;
int j=0;
int k=0;
while(i<2 || j<5)
{
    if (arr1[i]<arr2[j])
    {
    arr3[k]=arr1[i];
    i++;
    k++;
    }
    else
    {
         arr3[k]=arr2[j];
         j++;
         k++;
    }
}
for (int i=0;i<sizeof(arr3)/sizeof(arr3[0]);i++)
{
    cout<<arr3[i]<<" ";
}

    return 0;
}
